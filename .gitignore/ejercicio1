#include <iostream>
using namespace std;
int main()
{
    int n1,n2;
    cout<<"Ingresa el primer numero: "<<endl;
    cin>>n1;
    cout<<"Ingresa el segundo: "<<endl;
    cin>>n2;

        if(n1%n2==0)
            cout<<n1<<" es multiplo de "<<n2<<endl;
        else
            cout<<n1<<" no es multiplo de "<<n2<<endl;

    return 0;
}
